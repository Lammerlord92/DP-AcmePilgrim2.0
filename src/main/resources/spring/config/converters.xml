<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
	
		<bean class="converters.ActorToStringConverter"/>
		<bean class="converters.StringToActorConverter"/>
				
		<bean class="converters.AdministratorToStringConverter"/>
		<bean class="converters.StringToAdministratorConverter"/>
		
		<bean class="converters.AnecdoteToStringConverter"/>
		<bean class="converters.StringToAnecdoteConverter"/>
				
<!-- 		<bean class="converters.AssessmentToStringConverter"/>
		<bean class="converters.StringToAssessmentConverter"/>  -->
				
		<bean class="converters.AuthorityToStringConverter"/>
		<bean class="converters.StringToAuthorityConverter"/>
				
		<bean class="converters.ComplaintToStringConverter"/>
		<bean class="converters.StringToComplaintConverter"/>

<!-- 		<bean class="converters.ComplaintFormToStringConverter"/>
		<bean class="converters.StringToComplaintFormConverter"/> -->
		
		<bean class="converters.DiscussToStringConverter"/>
		<bean class="converters.StringToDiscussConverter"/>
						
		<bean class="converters.FolderToStringConverter"/>
		<bean class="converters.StringToFolderConverter"/>
		
		<bean class="converters.MessageToStringConverter"/>
		<bean class="converters.StringToMessageConverter"/>
				
		<bean class="converters.PilgrimToStringConverter"/>
		<bean class="converters.StringToPilgrimConverter"/>
		
		<bean class="converters.RegisterToStringConverter"/>
		<bean class="converters.StringToRegisterConverter"/>
		
		<bean class="converters.RouteToStringConverter"/>
		<bean class="converters.StringToRouteConverter"/>
				
		<bean class="converters.StageToStringConverter"/>
		<bean class="converters.StringToStageConverter"/>
		
		<bean class="converters.StageOrderToStringConverter"/>
		<bean class="converters.StringToStageOrderConverter"/>
				
		<bean class="converters.StageRatingToStringConverter"/>
		<bean class="converters.StringToStageRatingConverter"/>
				
		<bean class="converters.UserAccountToStringConverter"/>
		<bean class="converters.StringToUserAccountConverter"/>
		
		
		
	</util:list>
	
</beans>